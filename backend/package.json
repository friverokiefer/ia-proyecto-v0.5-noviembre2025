    "express": "^4.19.2",
    "openai": "^4.57.0",
    "sharp": "^0.33.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^22.18.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
